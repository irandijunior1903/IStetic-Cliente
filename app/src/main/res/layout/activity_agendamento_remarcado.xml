<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/fundo"
    android:clickable="true"
    android:focusable="auto"
    tools:context=".Agendamento1">

    <ImageView
        android:id="@+id/salvarAgendamento"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="127dp"
        android:layout_marginBottom="78dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fraseConfirmarAgendamento"
        app:srcCompat="@drawable/check" />

    <TextView
        android:id="@+id/fraseConfirmarAgendamento"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Seu agendamento foi alterado com sucesso, clique no Ã­cone abaixo para voltar ao perfil!"
        android:textColor="@color/corDoTexto"
        android:textSize="16sp"
        app:fontFamily="@font/open_sans_semibold"
        app:layout_constraintBottom_toTopOf="@+id/salvarAgendamento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView22" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="322dp"
        android:layout_height="182dp"
        android:layout_marginTop="125dp"
        app:layout_constraintBottom_toTopOf="@+id/fraseConfirmarAgendamento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_menor" />

</androidx.constraintlayout.widget.ConstraintLayout>